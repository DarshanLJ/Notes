    "import socket\n",
    "mysock=socket.socket()\n",
    "mysock.connect((\"https://data.pr4e.org/\",80))\n",
    "url=\"GEThttps://data.pr4e.org/romeo.txt HTTP/1.0\\r\\n\\r\\n\".encode()\n",
    "mysock.send(url)\n",
    "while True:\n",
    "    data=mysock.recv(512).decode()\n",
    "    if not data:\n",
    "        break\n",
    "    print(data,end=\"\")\n",
    "mysock.close()"
   ]
